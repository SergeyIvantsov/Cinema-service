<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.30.xsd">


    <changeSet id="create_dir" author="sergey">
        <createTable tableName="directors">
            <column name="id_director" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="directorName" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="directorSurname" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>

        </createTable>
    </changeSet>





</databaseChangeLog>
